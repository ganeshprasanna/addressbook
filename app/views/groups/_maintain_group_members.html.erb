<div align="center">
  <table id="maintainGroupTable">
    <tr>
      <th>Not Included</th>
      <th>&nbsp;</th>
      <th>Included</th>
    </tr>

    <tr>
      <td id="notIncludedListCell" class="groupListAddresses">
        <ul>
          <div id="notIncludedList">
            <%=
              @not_included.to_a.sort! { |x,y| x.compare_by_primary_contact(y) }
              select_tag(:not_included,
                options_for_group_members(@not_included),
                :multiple => true, :class => 'selectGroupAddresses', :onDblclick => 'includeSelectedMembers();')
            %>
          </div>
        </ul>
      </td>

      <td id="maintainGroupControls">
        <%= link_to('<<', '#', :class => 'exclude_all_members_link') %>
        <%= link_to('<', '#', :class => 'exclude_selected_members_link') %>
        <br/>
        <%= link_to('>', '#', :class => 'include_selected_members_link') %>
        <%= link_to('>>', '#', :class => 'include_all_members_link') %>
      </td>

      <td id="includedListCell" class="groupListAddresses">
        <ul>
          <div id="includedList">
            <%=
              @included.to_a.sort! { |x,y| x.compare_by_primary_contact(y) }
              select_tag(:included,
                options_for_group_members(@included),
                :multiple => true, :class => 'selectGroupAddresses', :onDblclick => 'excludeSelectedMembers();')
            %>
          </div>
        </ul>
      </td>
    </tr>
  </table>
</div>
