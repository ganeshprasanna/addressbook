<table class="addressOption">
  <tr>
    <td><label>Address Line 1:</label></td>
    <td><%= text_field 'address', 'address1', 'size' => 40 %></td>
  </tr>
  <tr>
    <td><label>Address Line 2:</label></td>
    <td><%= text_field 'address', 'address2', 'size' => 40 %></td>
  </tr>
  <tr>
    <td><label>City:</label></td>
    <td><%= text_field 'address', 'city', 'size' => 20 %></td>
  </tr>
  <tr>
    <td><label>State:</label></td>
    <td><%= text_field 'address', 'state', 'size' => 5 %></td>
  </tr>
  <tr>
    <td><label>Zip Code:</label></td>
    <td><%= text_field 'address', 'zip', 'size' => 15 %></td>
  </tr>
</table>
